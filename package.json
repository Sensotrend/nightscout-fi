{
  "name": "nightscout-fhir-connect",
  "version": "1.4.14",
  "description": "Nightscout REST API provider for FHIR PHRs",
  "repository": "https://github.com/sensotrend/nightscout-fi",
  "dependencies": {
    "@awaitjs/express": "^0.6.1",
    "@githubprimer/octicons-react": "^8.5.0",
    "@sendgrid/mail": "^7.4.2",
    "ajv": "^7.2.4",
    "async-csv": "^2.1.3",
    "axios": "^0.21.4",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-session": "^2.0.0-rc.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "esm": "^3.2.23",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "fhir-kit-client": "git://github.com/mrinnetmaki/fhir-kit-client.git#error-handling",
    "formik": "^2.2.6",
    "fs-extra": "^9.1.0",
    "jose": "^3.20.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "mongoose": "^5.12.2",
    "morgan": "^1.9.1",
    "nanoid": "^3.1.20",
    "node-cache": "^5.1.2",
    "node-sass": "^4.14.1",
    "openid-client": "^4.6.0",
    "promise-parallel-throttle": "^3.3.0",
    "qrcode": "^1.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.1",
    "sensotrend-converter": "git://github.com/Sensotrend/sensotrend-converter.git#nightscout",
    "simple-oauth2": "^4.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.2.1",
    "yargs": "^16.2.0"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.9"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "env-cmd": "^8.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "source-map-support": "^0.5.16",
    "supertest": "^6.1.3"
  },
  "esm": {
    "cjs": true,
    "sourceMap": false,
    "cache": false
  },
  "nyc": {
    "all": false,
    "sourceMap": false,
    "instrument": true,
    "cache": false,
    "include": [
      "lib/**/*.js",
      "*.js"
    ],
    "exclude": [
      "coverage/**",
      "packages/*/test/**",
      "test/**",
      "test{,-*}.js",
      "**/*{.,-}test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/babel.config.js"
    ],
    "require": [
      "esm"
    ]
  },
  "engines": {
    "node": "^12.2.0",
    "npm": "6.x"
  },
  "scripts": {
    "preinstall": "echo 'Removing node_modules to force clean install' && rm -rf node_modules",
    "_preinstall": "echo 'Removing node_modules to force clean install' && rm -rf node_modules && git submodule init && git submodule update --remote",
    "_postinstall": "react-scripts build",
    "build": "react-scripts build",
    "server": "env-cmd ./my.env nodemon esmloader.js",
    "client": "react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "ui": "react-scripts start",
    "prestart": "react-scripts build",
    "start": "node esmloader.js",
    "test": "env-cmd ./test.env react-scripts test --env=jsdom && env-cmd ./test.env mocha --exit --timeout 8000 --require esm tests/*.test.js",
    "coverage": "env-cmd ./test.env nyc mocha --exit --timeout 8000 --require esm tests/*.test.js",
    "coverage-html": "rm -rf .nyc_output && npm run-script coverage && nyc report --reporter html && nyc report --reporter text-lcov > coverage/codecov.log"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "ignore": [
      "uploads/*"
    ]
  }
}

{
  "name": "nightscout-fhir-connect",
  "version": "1.3.17-beta",
  "description": "Nightscout REST API provider for FHIR PHRs",
  "repository": "https://github.com/sensotrend/nightscout-fi",
  "dependencies": {
    "@awaitjs/express": "^0.6.1",
    "@githubprimer/octicons-react": "^8.5.0",
    "@sendgrid/mail": "^7.2.3",
    "ajv": "^6.12.3",
    "async-csv": "^2.1.3",
    "axios": "^0.19.2",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-session": "^2.0.0-rc.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.3",
    "esm": "^3.2.23",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fhir-kit-client": "git://github.com/mrinnetmaki/fhir-kit-client.git#error-handling",
    "forever": "^3.0.0",
    "formik": "^2.1.5",
    "fs-extra": "^9.0.1",
    "jose": "^1.28.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "mongoose": "^5.9.29",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.12",
    "node-cache": "^5.1.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "openid-client": "^3.15.9",
    "promise-parallel-throttle": "^3.3.0",
    "qrcode": "^1.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "sensotrend-converter": "git://github.com/Sensotrend/sensotrend-converter.git#beta",
    "simple-oauth2": "^4.1.0",
    "stjs": "0.0.5",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "yargs": "^15.4.1"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.9"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "env-cmd": "^10.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "source-map-support": "^0.5.19",
    "supertest": "^4.0.2",
    "touch": "^3.1.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7"
  },
  "esm": {
    "cjs": true,
    "sourceMap": false,
    "cache": false
  },
  "nyc": {
    "all": false,
    "sourceMap": false,
    "instrument": true,
    "cache": false,
    "include": [
      "lib/**/*.js",
      "*.js"
    ],
    "exclude": [
      "coverage/**",
      "packages/*/test/**",
      "test/**",
      "test{,-*}.js",
      "**/*{.,-}test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/babel.config.js"
    ],
    "require": [
      "esm"
    ]
  },
  "engines": {
    "node": "^12.2.0",
    "npm": "6.x"
  },
  "scripts": {
    "build": "react-scripts build",
    "client": "npm run enable-trusted-ssl && cross-env HTTPS=true react-scripts start",
    "copy-cert": "copyfiles server.pem node_modules/webpack-dev-server/ssl",
    "coverage": "env-cmd -f ./test.env nyc mocha --exit --timeout 8000 --require esm tests/*.test.js",
    "coverage-html": "rm -rf .nyc_output && npm run-script coverage && nyc report --reporter html && nyc report --reporter text-lcov > coverage/codecov.log",
    "delete-cert": "del-cli node_modules/webpack-dev-server/ssl/server.pem",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "enable-trusted-ssl": "npm run delete-cert && npm run copy-cert && npm run touch-cert",
    "postinstall": "react-scripts build",
    "server": "env-cmd -f ./my.env nodemon esmloader.js",
    "start": "node esmloader.js",
    "test": "env-cmd -f ./test.env react-scripts test --env=jsdom && env-cmd -f ./test.env mocha --exit --timeout 8000 --require esm tests/*.test.js",
    "touch-cert": "nodetouch node_modules/webpack-dev-server/ssl/server.pem"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "ignore": [
      "uploads/*"
    ]
  }
}
